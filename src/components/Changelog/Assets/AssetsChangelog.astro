---
import { UniQuery } from 'san-webkit-next/api/executor.js'
import { queryAssetsChangelog } from '../api'
import AssetsChangelogList from './AssetsChangelogList.svelte'

const PAGE_SIZE = 20
const initialData = await queryAssetsChangelog(UniQuery(fetch))(
  1,
  PAGE_SIZE,
  ''
)
---

<AssetsChangelogList client:load {initialData} pageSize={PAGE_SIZE} />
